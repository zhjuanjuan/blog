<h1>Listing commtents</h1>

<table>
  <tr>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @commtents.each do |commtent| %>
  <tr>
    <td><%= commtent.content %></td>
    <td><%= link_to 'Show', commtent %></td>
    <td><%= link_to 'Edit', edit_commtent_path(commtent) %></td>
    <td><%= link_to 'Destroy', commtent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

